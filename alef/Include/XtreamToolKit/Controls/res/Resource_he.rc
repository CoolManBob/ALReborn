XT_IDM_POPUP MENU DISCARDABLE 
BEGIN
    POPUP "RClickOutlookBar"
    BEGIN
        MENUITEM "סמלים קטנים ",                XT_IDC_SMALLICON
        MENUITEM "סמלים גדולים",                XT_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "בטל",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "גזור",                        ID_EDIT_CUT
        MENUITEM "העתק",                       ID_EDIT_COPY
        MENUITEM "הדבק",                      ID_EDIT_PASTE
        MENUITEM "מחיקה",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "בחר הכל",                 ID_EDIT_SELECT_ALL
    END
    POPUP "RClickHeader"
    BEGIN
        MENUITEM "מיין בסדר עולה",              XT_IDC_SORTASC
        MENUITEM "מיין בסדר יורד",             XT_IDC_SORTDSC
        MENUITEM SEPARATOR
        POPUP "יישור"
        BEGIN
            MENUITEM "ישר לשמאל",                  XT_IDC_ALIGNLEFT
            MENUITEM "ישר למרכז",                XT_IDC_ALIGNCENTER
            MENUITEM "ישר לימין",                 XT_IDC_ALIGNRIGHT
        END
    END
END

