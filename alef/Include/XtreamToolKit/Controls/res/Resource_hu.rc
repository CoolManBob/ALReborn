    BEGIN
        MENUITEM "Kicsi ikonok",                XT_IDC_SMALLICON
        MENUITEM "Nagy gombok",                XT_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "Visszavonás",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Kivágás",                        ID_EDIT_CUT
        MENUITEM "Másolás",                       ID_EDIT_COPY
        MENUITEM "Beillesztés",                      ID_EDIT_PASTE
        MENUITEM "Törlés",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "Mindet kijelöli",                 ID_EDIT_SELECT_ALL
    END
    POPUP "RClickHeader"
    BEGIN
        MENUITEM "Rendezés - növekvõ",              XT_IDC_SORTASC
        MENUITEM "Rendezés - csökkenõ",             XT_IDC_SORTDSC
        MENUITEM SEPARATOR
        POPUP "Igazítás"
        BEGIN
            MENUITEM "Balra zárás",                  XT_IDC_ALIGNLEFT
            MENUITEM "Igazítás középre",                XT_IDC_ALIGNCENTER
            MENUITEM "Jobbra zárás",                 XT_IDC_ALIGNRIGHT
        END
    END
END

