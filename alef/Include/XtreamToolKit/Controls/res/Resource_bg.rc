XT_IDM_POPUP MENU DISCARDABLE 
BEGIN
    POPUP "RClickOutlookBar"
    BEGIN
        MENUITEM "Малки икони",                XT_IDC_SMALLICON
        MENUITEM "Големи икони",                XT_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "Върни",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Премести в буфера",                        ID_EDIT_CUT
        MENUITEM "Копирай",                       ID_EDIT_COPY
        MENUITEM "Вмъкни",                      ID_EDIT_PASTE
        MENUITEM "Изтрий",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "Маркирай всичко",                 ID_EDIT_SELECT_ALL
    END
    POPUP "RClickHeader"
    BEGIN
        MENUITEM "Сортирай възходящо",              XT_IDC_SORTASC
        MENUITEM "Сортирай низходящо",             XT_IDC_SORTDSC
        MENUITEM SEPARATOR
        POPUP "Подравняване"
        BEGIN
            MENUITEM "Ляво",                  XT_IDC_ALIGNLEFT
            MENUITEM "Центрирано",                XT_IDC_ALIGNCENTER
            MENUITEM "Дясно",                 XT_IDC_ALIGNRIGHT
        END
    END
END

