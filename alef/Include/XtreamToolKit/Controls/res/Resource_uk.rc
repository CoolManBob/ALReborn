XT_IDM_POPUP MENU DISCARDABLE 
BEGIN
    POPUP "RClickOutlookBar"
    BEGIN
        MENUITEM "Дрібні значки",                XT_IDC_SMALLICON
        MENUITEM "Великі значки",                XT_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "Скасувати",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "видалення до буфера обміну",                        ID_EDIT_CUT
        MENUITEM "копіювання",                       ID_EDIT_COPY
        MENUITEM "Paste",                      ID_EDIT_PASTE
        MENUITEM "Видалити",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "виділення всіх об'єктів",                 ID_EDIT_SELECT_ALL
    END
    POPUP "RClickHeader"
    BEGIN
        MENUITEM "Сортування за зростанням",              XT_IDC_SORTASC
        MENUITEM "Сортування за спаданням",             XT_IDC_SORTDSC
        MENUITEM SEPARATOR
        POPUP "Вирівнювання"
        BEGIN
            MENUITEM "За лівим краєм",                  XT_IDC_ALIGNLEFT
            MENUITEM "Вирівняти по центру",                XT_IDC_ALIGNCENTER
            MENUITEM "За правим краєм",                 XT_IDC_ALIGNRIGHT
        END
    END
END

