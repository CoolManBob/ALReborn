    BEGIN
        MENUITEM "Võta tagasi",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Lõika",                        ID_EDIT_CUT
        MENUITEM "Kopeeri",                       ID_EDIT_COPY
        MENUITEM "Kleebi",                      ID_EDIT_PASTE
        MENUITEM "Kustuta",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "Vali kõik",                 ID_EDIT_SELECT_ALL
