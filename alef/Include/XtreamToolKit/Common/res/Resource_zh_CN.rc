XTP_IDR_MENU_HEADERFOOTER_FORMATS MENU DISCARDABLE 
BEGIN
    POPUP "HF"
    BEGIN
        MENUITEM "&&d   短日期格式",    XTP_ID_HF_FORMAT_D1
        MENUITEM "&&D   长日期格式",     XTP_ID_HF_FORMAT_D2
        MENUITEM SEPARATOR
        MENUITEM "&&t   时间",                 XTP_ID_HF_FORMAT_T1
        MENUITEM "&&T   时间 (24 小时制)",       XTP_ID_HF_FORMAT_T2 
        MENUITEM SEPARATOR
        MENUITEM "&&p   页码",          XTP_ID_HF_FORMAT_P1
        MENUITEM "&&P   页数",      XTP_ID_HF_FORMAT_P2
        MENUITEM SEPARATOR
        MENUITEM "&&b   左对齐 &&b 居中 &&b 右对齐", XTP_ID_HF_FORMAT_B
        MENUITEM SEPARATOR
        MENUITEM "&&w   窗口标题",         XTP_ID_HF_FORMAT_W
        MENUITEM "&&&&  符号 (&&)",       XTP_ID_HF_FORMAT_UMP
        MENUITEM "\\n   新行",             XTP_ID_HF_FORMAT_N
    END
END

