    PUSHBUTTON      "Menos zoom",AFX_ID_PREVIEW_ZOOMOUT,232,2,44,12,BS_FLAT
    PUSHBUTTON      "Fechar",AFX_ID_PREVIEW_CLOSE,278,2,44,12,BS_FLAT
END


