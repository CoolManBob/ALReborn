XTP_IDR_MENU_HEADERFOOTER_FORMATS MENU DISCARDABLE 
BEGIN
    POPUP "HF"
    BEGIN
        MENUITEM "&&d   Formato de data abreviada",    XTP_ID_HF_FORMAT_D1
        MENUITEM "&&D   Formato de data por extenso",     XTP_ID_HF_FORMAT_D2
        MENUITEM SEPARATOR
        MENUITEM "&&t   Tempo",                 XTP_ID_HF_FORMAT_T1
        MENUITEM "&&T   Tempo (24 horas)",       XTP_ID_HF_FORMAT_T2 
        MENUITEM SEPARATOR
        MENUITEM "&&p   Número da Página",          XTP_ID_HF_FORMAT_P1
        MENUITEM "&&P   Número de páginas",      XTP_ID_HF_FORMAT_P2
        MENUITEM SEPARATOR
        MENUITEM "&&b   Alinhamento à esquerda &&b Centralizar &&b Alinhamento à direita", XTP_ID_HF_FORMAT_B
        MENUITEM SEPARATOR
        MENUITEM "&&w   Título da janela",         XTP_ID_HF_FORMAT_W
        MENUITEM "&&&&  E comercial (&&)",       XTP_ID_HF_FORMAT_UMP
        MENUITEM "\\n   Nova linha",             XTP_ID_HF_FORMAT_N
    END
END

